set nocompatible
colorscheme gruvbox
let g:python3_host_prog='/usr/bin/python3'
let g:airline_theme='minimalist'
autocmd TermOpen * setlocal nonumber norelativenumber 
set linebreak
set number
set tabstop=4
set softtabstop=4
set expandtab
set shiftwidth=4
set autoindent
set wildmode=longest,list
filetype plugin indent on
syntax on
